      // value              $internalField;
    }
    door
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    windowsmall
    {
        type               fixedFluxPressure;
        value              $internalField;

    }
    windowbig
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    columns
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    ceiling
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    floor
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    wallwindow
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    walldoor
    {
        type               fixedFluxPressure;
        value              $internalField;
    }
    walloutside
    {
